{
  "name": "myusic-bot",
  "version": "1.0.0",
  "description": "A discord bot that can play music in typescript",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "lint": "eslint src/**/*.{ts,json}",
    "lint:fix": "eslint --fix src/**/*.{ts,json}",
    "format": "prettier --write src/**/*.{ts,json} --config ./.prettierrc"
  },
  "keywords": [
    "discord",
    "music",
    "bot"
  ],
  "author": "Agustinus Nicander Hery",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "ts-node": "^10.9.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint"
    }
  }
}
